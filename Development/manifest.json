{
    "manifest_version": 2,
	"name": "TweetNinja Autofill/ACO",
    "description":"Autofill/ACO developed by TweetNinja",
    "version": "3.0",
    "icons": {"128": "./icons/icon_128.png"},
    "browser_action":{
        "default_icon": "./icons/icon.png",
        "default_popup": "./popup_pages/popup.html"
    },
    "content_security_policy": "script-src 'self' https://www.tweet-ninja.com; object-src 'self'",
	"permissions": ["activeTab", "storage", "background", "tabs", "<all_urls>", "https://www.tweet-ninja.com/", "clipboardWrite", "clipboardRead"],
	"background": {
		"scripts": ["background.js"]
    },
    "options_page": "./option_page/clicks.html",
    "content_scripts": [
        {
			"matches": [
				"https://*/*", "http://*/*"
			],
			"exclude_matches": [
				"https://*/cart",
				"https://*/*/checkouts/*",
				"https://*.twitter.com/*",
				"https://twitter.com/*",
				"https://www.youtube.com/*",
				"https://www.google.com/*",
				"https://www.facebook.com/*",
				"https://checkout.stripe.com/*",
				"https://checkout.stripe.com/pay/*",
				"https://checkout.stripe.dev/*",
				"https://*/purchase",
				"https://*/purchase*"
			],
			"js": [
				"scripts/shopifyatc.js",
				"scripts/inject.js",
				"scripts/animationremover.js",
				"scripts/ducky.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://*/*", "http://*/*"
			],
			"js": [
				"scripts/customClick.js"
			],
			"all_frames": true
		},
		{
			"matches":[
				"https://*/purchase",
				"https://*/purchase*"
			],
			"js":[
				"scripts/metalabs.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://*/cart"
			],
			"js": [
				"scripts/shopifycart.js"
			],
			"all_frames": true
		},
        {
			"matches": [
				"https://*/*/checkouts/*"
			],
			"js": [
				"scripts/shopify.js"
			]
        },
        {
			"matches": [
				"https://*.shopifycs.com/*"
			],
			"js": [
				"scripts/shopify2.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://js.stripe.com/*",
				"https://*.stripe.com/*",
				"https://checkout.stripe.dev/*"
			],
			"exclude_matches": [
				"https://*/purchase",
				"https://*/purchase*"
			],
			"js": [
				"scripts/stripe.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://checkout.stripe.com/*"
			],
			"js": [
				"scripts/stripev2.js"
			],
			"exclude_matches": [
				"https://checkout.stripe.com/pay/*"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://checkout.stripe.com/pay/*",
				"https://checkout.stripe.com/*"
			],
			"js": [
				"scripts/stripecheckout.js"
			],
			"exclude_matches": [
				"https://checkout.stripe.dev/*"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://www.supremenewyork.com/checkout"
			],
			"js": [
				"scripts/supreme.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://docs.google.com/forms/*"
			],
			"js": [
				"scripts/googleforms.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://*/*", "http://*/*"
			],
			"exclude_matches": [
				"https://*/purchase",
				"https://*/purchase*"
			],
			"js": [
				"scripts/veloaco.js",
				"scripts/shreyaco.js"
			],
			"all_frames": true
		},
		{
			"matches": [
			  "https://www.tldashboards.com/*"
			],
			"js": [
			  "scripts/tl_dashboard.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://www.google.com/recaptcha/*"
			],
			"js": [
				"scripts/captcha.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://*/checkout"
			],
			"js": [
				"scripts/bigcartel.js"
			],
			"all_frames": true
		},
		{
			"matches": [
				"https://www.supremenewyork.com/"
			],
			"js": [
				"scripts/monitoring.js"
			],
			"all_frames": true
        },
		{
			"matches": [
				"https://www.supremenewyork.com/shop/*"
			],
			"js": [
				"scripts/addtocart.js"
			],
			"all_frames": true
        }
    ]
}
